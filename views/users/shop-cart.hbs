    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index.html" rel="nofollow">Home</a>
                    <span></span> Shop
                    <span></span> Your Cart
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                          {{#if cartItems}}
                            <table class="table shopping-summery text-center clean">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col">Image</th>
                                        <th scope="col">Name</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Subtotal</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each cartItems.products}}
                                    <tr>
                                        <td class="image product-thumbnail"><img src="/images/{{product_id}}.jpg" alt="#"></td>
                                        <td class="product-des product-name">
                                            <h5 class="product-name"><a href="shop-product-right.html">{{product_title}}</a></h5>
                                            <p class="font-xs">Maboriosam in a tonto nesciung eget<br> distingy magndapibus.
                                            </p>
                                        </td>
                                        <td class="price" data-title="Price"><span>${{offerPrice}}</span></td>
                                        <td class="text-center" data-title="Stock">
                                            <div class="detail-qty border radius m-auto">
                                                {{!-- <a class="qty-down" onclick="changeQuantity('{{_id}}','{{product._id}}',1)"><i class="fi-rs-angle-small-down"></i></a>
                                                <span >{{quantity}}</span>
                                                <a  class="qty-up" onclick="changeQuantity('{{_id}}','{{product._id}}',-1)"><i class="fi-rs-angle-small-up"></i></a> --}}

                                                <button class=""  style="border: none;background: none;" onclick="changeQuantity('{{../cartId}}','{{product_id}}','{{../user._id}}',-1)"><i class="fi-rs-angle-small-down"></i></button>
                                                 <p class="" id="{{product_id}}" >{{quantity}}</p>
                                                 <button class="" style="border: none;background: none;"  onclick="changeQuantity('{{../cartId}}','{{product_id}}','{{../user._id}}',1)"><i class="fi-rs-angle-small-up"></i></button>
                                            </div>
                                        </td>
                                        <td class="text-right" data-title="Cart">
                                            <span id="{{product_id}}-subtotal">&#36;{{offerSubtotal}}</span>
                                        </td>
                                        <td class="action" data-title="Remove">
                                            {{!-- <a href="#" class="text-muted"><i class="fi-rs-trash"></i></a> --}}
                                           <button class="text-muted" style="border: none;background: none;" onclick="setModalData('{{../cartId}}','{{product_id}}')" data-bs-toggle="modal" data-bs-target="#exampleModal"><i class="fi-rs-trash text-danger"></i></button>
                                            </td>
                                    </tr>
                                    {{/each}}
                                    <tr>
                                        <td colspan="6" class="text-end">
                                            <a href="#" class="text-muted"> <i class="fi-rs-cross-small"></i> Clear Cart</a>
                                        </td>
                                    </tr>
                                    
                                </tbody>
                            </table>
                            {{else}}
                                    <div>
                                        <p>Your cart is empty please add some products into cart</p>
                                    </div>
                                    {{/if}} 
