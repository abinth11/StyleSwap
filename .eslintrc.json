{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": "error",
    "import/named": "error",
    "import/default": "error",
    "import/namespace": "error",
    "no-multi-spaces": ["error"],
    "semi": ["error", "never", { "beforeStatementContinuationChars": "always" }]

  },
  "plugins": [
    "import"
  ]
}
